# Example environment variables for mbx-inventory

# AirTable Configuration
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here

# Baserow Configuration
BASEROW_API_KEY=your_baserow_api_key_here
BASEROW_BASE_URL=https://api.baserow.io
BASEROW_DATABASE_ID=your_database_id_here

# NocoDB Configuration
NOCODB_API_KEY=your_nocodb_api_key_here
NOCODB_BASE_URL=https://your-nocodb-instance.com
NOCODB_PROJECT_ID=your_project_id_here

# PostgreSQL Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=mesonet_inventory
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password